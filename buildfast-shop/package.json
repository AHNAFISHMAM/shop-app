{
  "name": "buildfast-shop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "verify:feature-flags": "node scripts/verify-feature-flags.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "typecheck:watch:preserve": "tsc --noEmit --watch --preserveWatchOutput",
    "typecheck:fast": "tsc --noEmit --skipLibCheck",
    "typecheck:build": "tsc --build",
    "typecheck:build:watch": "tsc --build --watch",
    "typecheck:errors": "tsc --noEmit 2>&1 | Select-String 'error TS'",
    "typecheck:count": "powershell -Command \"tsc --noEmit 2>&1 | Select-String 'error TS' | Measure-Object | Select-Object -ExpandProperty Count\"",
    "typecheck:group": "node scripts/group-errors.js",
    "typecheck:by-file": "node scripts/errors-by-file.js",
    "typecheck:by-type": "node scripts/errors-by-type.js",
    "fix:auto": "node scripts/auto-fix-common.js",
    "fix:parallel": "node scripts/parallel-fix.js",
    "fix:type-imports": "node scripts/convert-type-imports.js",
    "setup:agents": "node scripts/setup-agent-coordination.js",
    "verify:setup": "node scripts/verify-agent-setup.js",
    "batch:generate": "powershell -ExecutionPolicy Bypass -File scripts/parallel-batch-processor.ps1",
    "batch:run": "powershell -ExecutionPolicy Bypass -File scripts/run-batch.ps1",
    "preflight:check": "node scripts/pre-flight-check.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.2.0",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/react-query": "^5.62.0",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.560.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-aria-components": "^1.13.0",
    "react-calendar": "^6.0.0",
    "react-day-picker": "^9.12.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^15.2.11",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^4.1.16",
    "vite": "^5.4.10",
    "vitest": "^4.0.8",
    "web-vitals": "^4.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
